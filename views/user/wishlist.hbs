<div><img src="/stylesheets/img/slider/shop.jpg" alt=""></div>
<div style="text-align:center;"><h1>WISHLIST</h1></div>
    <div class="f_cart_area pt-110 mb-100">
        <div class="container">
            <div class="row">
 {{#if products}}
                <div class="col-xl-8 col-lg-8 col-md-12">
                    <div class="cart_table">
                        <table>
                            <tr> 
                                <td>Product</td>
                                <td>product Name</td>
                                <td>price</td>
                                <td>Option</td>
                                
                               </tr>
                            <tbody>
                                {{#each products}}
                                <tr class="max-width-set">
                                    
                                    <td>
                                        <img style="width: 100px; height:auto;" src="{{this.products.image.[0]}}"  alt="">
                                    </td>
                                    <td>{{this.products.brand}}</td>
                                    {{#if this.products.offer}}
                                    <td>₹{{this.products.offer}} 
                                    </td>
                                  {{else}}
                                    <td>₹{{this.products.price}} 
                                    </td>
                                     {{/if}}
                                    <td class="width-set">
                                        {{#if this.products.available}}
                                        <button class="quantity btn btn-danger" onclick="removeWishlistItem('{{this._id }}','{{this.products._id}}')">Remove</button>
                                         <button class="quantity btn btn-danger" onclick="addtocart('{{this._id}}')">Add cart</button>
                                    {{else}}
                                                <p class="h5 text-danger">Sorry, this product is currently out of stock</p>
                                                 <button class="quantity btn btn-danger" onclick="removeWishlistItem('{{this._id }}','{{this.products._id}}')">Remove</button>
                                   {{/if}}
                                    </td>
                                    
                                </tr>
                                {{/each}} 
                            </tbody>
                          
                        </table>
                    </div>
                </div>
             {{else}}
                      <h1>WISHLIST EMPTY</h1>
                      {{/if}}
                </div>
            </div>
        </div>
    </div>
    
